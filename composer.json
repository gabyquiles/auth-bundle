{
  "name": "gabyquiles/auth-bundle",
  "version": "1.1.1",
  "license": "Apache-2.0",
  "keywords": [
    "aws",
    "cognito",
    "authentication"
  ],
  "minimum-stability": "dev",
  "description": "Provide the verification for jwt from AWS Cognito",
  "type": "library",
  "authors": [
    {
      "name": "Gabriel A. Quiles-Perez",
      "email": "info@gabyquiles.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "symfony/framework-bundle": "^3.4|^4.0",
    "symfony/security-bundle": "^3.4|^4.0",
    "lexik/jwt-authentication-bundle": "^2.6",
    "codercat/jwk-to-pem": " 0.0.3",
    "symfony/http-client": "^4.4"
  },
  "require-dev": {
    "symfony/phpunit-bridge": "^3.4|^4.0",
    "symfony/browser-kit": "^3.4|^4.0",
    "symfony/console": "^3.4|^4.0",
    "symfony/dom-crawler": "^3.4|^4.0",
    "symfony/var-dumper": "^3.4|^4.0",
    "symfony/yaml": "^3.4|^4.0",
    "friendsofphp/php-cs-fixer": "^1.1|^2.8"
  },
  "autoload": {
    "psr-4": {
      "GabyQuiles\\Auth\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "GabyQuiles\\Auth\\Test\\": "tests/"
    }
  }
}
